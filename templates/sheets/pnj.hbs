<form class="pnj" autocomplete="off">
  {{!log "pj" this}}
  <section class="sheet-body pj-sheet">
    {{> "systems/magna/templates/sheets/partials/pnj-header.hbs" }}
    {{#if locked}}
    <div class="sheet-cont">
          {{> "systems/magna/templates/sheets/partials/pnj-light.hbs"}}
      </div>
    {{else}}
    <div class="sheet-cont">
      {{! DÃ©tails Tab }}
      <div class="tab tab-competences flexcol" data-group="primary" data-tab="competences">
          {{> "systems/magna/templates/sheets/partials/pj-competences.hbs"}}
      </div>
      {{! pouvoirs Tab }}
      <div class="tab tab-pouvoirs flexcol" data-group="primary" data-tab="pouvoirs">
          {{> "systems/magna/templates/sheets/partials/pj-pouvoirs.hbs"}}
      </div>
      {{! Notes Tab }}
      <div class="tab tab-desc flexcol" data-group="primary" data-tab="description">
        {{> "systems/magna/templates/sheets/partials/pj-description.hbs"}}
      </div>
    </div>
    {{/if}}
  </section>
</form>